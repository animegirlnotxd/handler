{
   "name": "stackoverflow handler",
   "version": "1.0.0",
   "description": "stackoverflow handler project",
   "manifest_version": 2,
   "permissions": [
    "contextMenus"
    ],
   "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"]
    }
    ],
   "background": {
      "scripts": ["js/contextMenus.js", "js/background.js"],
      "persistent": false
   }
}